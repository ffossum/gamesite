{
  "name": "koa-example",
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "start": "node node_modules/forever/bin/forever --minUptime 1000 --spinSleepTime 1000 -c \"node --harmony\" ./dist/server.js",
    "build-server": "node node_modules/webpack/bin/webpack --colors --display-error-details --config configs/webpack.server.js",
    "build-client": "node node_modules/webpack/bin/webpack --colors --display-error-details --config configs/webpack.client.js",
    "build": "node node_modules/concurrently/src/main \"npm run build-server\" \"npm run build-client\"",
    "watch-server": "node node_modules/webpack/bin/webpack --watch --verbose --colors --display-error-details --config configs/webpack.server-watch.js",
    "watch-server-start": "node node_modules/just-wait --pattern \"dist/*.js\" && npm run start",
    "watch-client": "node node_modules/webpack-dev-server/bin/webpack-dev-server --config configs/webpack.client-watch.js",
    "watch": "node node_modules/concurrently/src/main --kill-others \"npm run watch-server-start\" \"npm run watch-server\" \"npm run watch-client\"",
    "lint": "node node_modules/eslint/bin/eslint src",
    "build-test": "node node_modules/webpack/bin/webpack --colors --config configs/webpack.test.js",
    "watch-build-test": "node node_modules/webpack/bin/webpack --colors --config configs/webpack.test-watch.js",
    "test": "npm run lint && npm run coverage",
    "coverage": "npm run build-test && (cd test && node ../node_modules/istanbul/lib/cli.js cover ../node_modules/mocha/bin/_mocha test.js)",
    "watch-test": "(cd test && mocha test.js --watch)"
  },
  "author": "Fredrik Fossum <fredrik.fossum@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^0.8.5",
    "bluebird": "^3.3.3",
    "classnames": "^2.2.3",
    "cookie": "^0.2.3",
    "forever": "^0.15.1",
    "history": "^2.0.0",
    "immutable": "3.7.6",
    "isomorphic-fetch": "^2.2.1",
    "jsonwebtoken": "^5.5.4",
    "koa": "^2.0.0",
    "koa-bodyparser": "^3.0.0",
    "koa-convert": "^1.2.0",
    "koa-passport": "^2.0.1",
    "koa-router": "^7.0.1",
    "koa-static": "^2.0.0",
    "lodash": "4.9.0",
    "moment": "2.12.0",
    "normalize.css": "^4.0.0",
    "passport-local": "^1.0.0",
    "postcss-import": "^8.0.2",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-redux": "^4.1.0",
    "react-router": "^2.0.0",
    "react-router-redux": "^4.0.0",
    "redux": "^3.1.1",
    "redux-immutablejs": "0.0.8",
    "redux-thunk": "^2.0.1",
    "shortid": "^2.2.4",
    "socket.io": "^1.4.5",
    "socket.io-client": "^1.4.5",
    "validator": "^5.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.4.5",
    "babel-eslint": "^6.0.0-beta.6",
    "babel-loader": "^6.2.1",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.1",
    "babel-preset-stage-0": "^6.3.13",
    "chai": "^3.5.0",
    "concurrently": "^1.0.0",
    "coveralls": "^2.11.9",
    "css-loader": "^0.23.1",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-react": "^4.2.1",
    "glob": "^7.0.0",
    "istanbul": "^1.0.0-alpha.2",
    "just-wait": "^1.0.3",
    "mocha": "^2.4.5",
    "postcss-cssnext": "^2.4.0",
    "postcss-loader": "^0.8.1",
    "redux-devtools": "^3.2.0",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "1.0.7",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.12",
    "webpack-dev-server": "^1.14.1"
  }
}
